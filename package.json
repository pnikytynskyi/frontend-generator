{
  "name": "test",
  "version": "0.0.1",
  "description": "Sample app",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "build:prod": "APP_CONFIG=production yarn run build",
    "build:prod:bundleanalyze": "WEBPACK_PLUGINS='bundleanalyze' yarn run build:prod",
    "serve:prod": "yarn run build && live-server ./public",
    "lint": "eslint .; exit 0",
    "lint:fix": "eslint . --fix; exit 0",
    "start": "webpack-dev-server --hot",
    "test": "jest",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "styleguide": "yarn styleguidist server",
    "styleguide:build": "yarn styleguidist build",
    "manage:translations": "node ./translationRunner.js"
  },
  "husky": {
    "hooks": {}
  },
  "lint-staged": {
    "*.{js, jsx},!blueprints,!prettierrc.js,!eslintrc.js": [
      "prettier --write",
      "git add",
      "eslint --max-warnings=0"
    ]
  },
  "jest": {
    "testURL": "http://localhost",
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.js"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/blueprints/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.1",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.0.0",
    "babel-loader": "8.0.5",
    "babel-plugin-module-resolver": "3.1.3",
    "babel-plugin-react-intl": "3.0.1",
    "cypress": "3.1.0",
    "eslint": "4.19.1",
    "eslint-config-prettier": "3.1.0",
    "eslint-config-react-app": "2.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "21.25.1",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "2.7.0",
    "eslint-plugin-react": "7.8.2",
    "file-loader": "1.1.11",
    "husky": "1.0.1",
    "jest": "24.0.0",
    "lint-staged": "7.3.0",
    "live-server": "1.2.1",
    "prettier": "1.14.3",
    "react-intl-translations-manager": "5.0.3",
    "react-styleguidist": "7.3.11",
    "react-test-renderer": "16.4.2",
    "redux-devtools-extension": "2.13.5",
    "redux-logger": "3.0.6",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.9.1",
    "webpack-blocks": "1.0.0",
    "webpack-cli": "3.3.12",
    "webpack-dashboard": "2.0.0",
    "webpack-dev-server": "3.1.4",
    "webpack-merge": "4.1.2",
    "ws": "6.0.0"
  },
  "dependencies": {
    "connected-react-router": "6.3.0",
    "date-fns": "1.29.0",
    "deepmerge": "2.1.1",
    "get-value": "3.0.1",
    "handlebars-webpack-plugin": "^1.6.0",
    "prop-types": "15.6.1",
    "qs": "6.5.2",
    "query-string": "6.2.0",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-intl": "2.4.0",
    "react-redux": "6.0.0",
    "react-router-dom": "4.2.2",
    "react-select": "2.0.0",
    "react-transition-group": "2.5.3",
    "redux": "4.0.0",
    "redux-actions": "2.4.0",
    "redux-api-middleware": "2.3.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-thunk": "2.3.0",
    "reselect": "3.0.1",
    "styled-components": "3.3.3"
  }
}
